{
  "comments": {
    "lineComment": ";",
    "blockComment": ["#|", "|#"]
  },
  "brackets": [
    ["(", ")"]
  ],
  "autoClosingPairs": [
    { "open": "(", "close": ")" },
    { "open": "\"", "close": "\"", "notIn": ["string"] },
    { "open": "#|", "close": "|#", "notIn": ["string"] }
  ],
  "surroundingPairs": [
    ["(", ")"],
    ["\"", "\""]
  ],
  "indentationRules": {
    "increaseIndentPattern": "^\\s*\\(.*$",
    "decreaseIndentPattern": "^\\s*\\)\\s*$"
  },
  "wordPattern": "[\\w+\\-<>/*&=.?!$%:@\\[\\]^{}~#|]+"
}
